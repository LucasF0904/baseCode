    "joi": "^17.5.0",
    "joi-date-extensions": "^1.2.0",
    "joi-translation-pt-br": "^0.0.6",
    "jsdoc": "^3.6.7",
    "jsonwebtoken": "^8.5.1",
    "maskdata": "^1.1.6",
    "moment": "^2.29.1",
    "mongodb": "^3.7.3",
    "mongoose": "^6.1.9",
    "morgan": "^1.10.0",
    "multer": "^1.4.3",
    "mysql": "^2.18.1",
    "nodemailer": "^6.7.0",
    "nodemon": "^2.0.15",
    "pg": "^8.7.1",
    "pino": "^7.6.3",
    "pretty-error": "^4.0.0",
    "rate-limiter-flexible": "^2.3.4",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "sql.js": "^1.6.2",
    "supertest": "^6.1.6",
    "tsyringe": "^4.6.0",
    "typeorm": "^0.2.45",
