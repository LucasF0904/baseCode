{
  "name": "basecode",
  "version": "1.0.0",
  "description": "Base code in Typescript, using Node.js with tests",
  "main": "index.js",
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/node": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-proposal-decorators": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-typescript": "^7.16.0",
    "@faker-js/faker": "^6.0.0-beta.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/http-errors": "^1.8.2",
    "@types/ioredis": "^4.28.1",
    "@types/jest": "^27.0.3",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.10.2",
    "@types/nodemailer": "^6.4.4",
    "@types/pino": "^7.0.5",
    "@types/redis": "^2.8.32",
    "@types/uuid": "^8.3.3",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "babel-jest": "^27.3.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-prettier": "3.2.0",
    "jest": "^27.4.3",
    "pino-pretty": "^7.3.0",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.5.5"
  },
  "scripts": {
    "dev-old": "tsnd --inspect --transpile-only --ignore-watch node_modules -r tsconfig-paths/register src/shared/http/server.ts ts-node | pino-pretty -c",
    "dev2": "nodemon --watch 'src/' --exec 'ts-node -r tsconfig-paths/register src/shared/http/server.ts' -e ts",
    "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/shared/http/server.ts",
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --ext .ts --fix",
    "typeorm": "tsnd -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "prettier-check": "prettier -c ./src",
    "prettier-write": "prettier -w ./src",
    "eslint-check": "eslint --ext .ts ./src",
    "fix-code": "prettier -w 'src/*/.ts' && eslint --fix --ext .ts ./src",
    "prepare": "husky install",
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files",
    "test": "jest --verbose --runInBand",
    "teste": "jest --passWithNoTests --noStackTrace --runInBand --detectOpenHandles",
    "seed": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed"
  },
  "dependencies": {
    "@joi/date": "^2.1.0",
    "@meanie/mongoose-to-json": "^2.6.0",
    "@types/axios": "^0.14.0",
    "@types/faker": "^6.6.9",
    "@types/helmet": "^4.0.0",
    "@types/moment": "^2.13.0",
    "@types/morgan": "^1.9.3",
    "@types/supertest": "^2.0.11",
    "aws-sdk": "^2.830.0",
    "axios": "^0.24.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "celebrate": "^15.0.0",
    "class-transformer": "^0.5.1",
    "clean-deep": "^3.4.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "es6-shim": "^0.35.6",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.25.2",
    "express": "^4.17.2",
    "express-async-errors": "^3.1.1",
    "faker": "^5.5.3",
    "fast-glob": "^3.2.11",
    "handlebars": "^4.7.7",
    "helmet": "^4.6.0",
    "http-errors": "^2.0.0",
    "husky": "^7.0.4",
